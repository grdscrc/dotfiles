set editing-mode vi
set blink-matching-paren on
# GRAY='\033[1;30m'
# NC='\033[0m' # No Color
set show-mode-in-prompt on
set vi-ins-mode-string "\1\033[1;30m\2|\1\033[0m\2ins "
set vi-cmd-mode-string ":\1\033[1;30m\2cmd\1\033[0m\2 "
"jk": vi-movement-mode
"\C-x\C-x": re-read-init-file
"\C-l": clear-screen
"\C-a": beginning-of-line
"\C-e": end-of-line
"\C-gg": "git "
"\C-gp": "git pull"
"\C-gs": "git status"
"\C-p": previous-history
"\C-n": next-history
